{
  "name": "tsconfig-aliases",
  "description": "Parse aliases from tsconfig.json for bundler configurations.",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "import": "./out/index.js",
      "default": "./out/index.js"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "review": "prettier --check . && oxlint",
    "build": "rolldown --config rolldown.config.ts",
    "test": "vitest run",
    "dev": "vitest"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "oxlint": "^1.22.0",
    "prettier": "^3.6.2",
    "prettier-plugin-sh": "^0.18.0",
    "rolldown": "1.0.0-beta.42",
    "rolldown-plugin-dts": "^0.16.11",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
